{
  "name": "@paintable/vue",
  "version": "0.1.0",
  "description": "Paintable for vue",
  "author": "Ph1p <me@ph1p.dev>",
  "types": "index.d.ts",
  "keywords": [
    "canvas",
    "vue",
    "paintable",
    "touch",
    "mouse",
    "draw",
    "plugin",
    "component"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "npm run build && vue-cli-service serve",
    "build": "vue-cli-service build --mode production --target lib --name paintable ./src/plugin.ts && rm ./dist/demo.html",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build",
    "vercel-build": "npm run build && vue-cli-service build ./src/main.ts --dest ./demo"
  },
  "main": "dist/paintable.common.js",
  "module": "dist/paintable.umd.min.js",
  "files": [
    "dist/paintable.common.js",
    "dist/paintable.umd.min.js",
    "dist/paintable.umd.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "typescript": "~3.9.3"
  },
  "peerDependencies": {
    "@paintable/core": "0.x"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "prettier": {
    "singleQuote": true
  },
  "gitHooks": {
    "pre-commit": "npm run build && lint-staged"
  },
  "lint-staged": {
    "./src/*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ph1p/paintable-vue"
  }
}
